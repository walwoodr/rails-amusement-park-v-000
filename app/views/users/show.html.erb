<div class="center">
  <div class="pic">
    <img src="https://randomuser.me/api/portraits/women/28.jpg" alt="woman in hat">
  </div>
  <div class="info">
    <p><strong>Name:</strong> <%= @user.name %></p>
    <%= content_tag :p, "ADMIN" if @user.admin %>
    <p><strong>Mood:</strong> <%= @user.mood %></p>
    <p><strong>Height:</strong> <%= @user.height %> inches</p>
    <p><strong>Tickets:</strong> <%= @user.tickets %></p>
    <p><%= link_to "Edit", edit_user_path(@user) %></p>
    <p><%= link_to "See attractions", attractions_path %></p>
  </div>
</div>
